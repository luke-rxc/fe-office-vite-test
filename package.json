  "scripts": {
    "start": "vite --host manager-office-local.prizm.co.kr --port 4000",
    "start:partner": "vite --host partner-office-local.prizm.co.kr --mode stage --port 4000",
    "build": "yarn run build:production",
    "build:develop": "vite build --mode development --debug",
    "build:stage": "vite build --mode stage --debug",
    "build:canary": "vite build --mode canary --debug",
    "build:production": "vite build --mode production --debug",
    "build:stats": "webpack-bundle-analyzer ./build/bundle-stats.json",
    "test": "vite test",
    "eject": "react-scripts eject",
    "generate": "plop",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --ext .tsx,.ts,.js --quiet src/",
    "lint:fix": "eslint --fix --ext .tsx,.ts,.js src/",
    "stylelint": "stylelint './src/**/*.{ts,tsx,js,jsx,css,scss}' '!./src/**/*.d.ts' --syntax 'css-in-js'",
    "stylelint:fix": "stylelint './src/**/*.{ts,tsx,js,jsx,css,scss}' '!./src/**/*.d.ts' --fix --syntax 'css-in-js'",
    "server": "npx json-server ./mock_up/data.json --port 5000",
    "postinstall": "husky install",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
